{{#liquid-if show}}
<div class="row sr-panel">
  <div class="col-3 sr-checklist">
  <div><span class="sr-checklist-title">Google News Validation</span></div>
  <span class="sr-gn-grade fail">FAIL</span>
    {{#each googleNews as |rule|}}
      <p>
        <i class="icon-check"></i>
        <span class="sr-rule-title">{{rule.title}}</span>
      </p>
    {{/each}}
  </div>
  <div class="col-3 sr-checklist val">
  <div><span class="sr-checklist-title">SEO Validation</span></div>
    {{#each seoA as |rule|}}
      <p>
        <i class="icon-check"></i>
        <span class="sr-rule-title">{{rule.title}}</span>
        <span class="sr-rule-score">{{rule.score}}</span>
      </p>
    {{/each}}
  </div>
  <div class="col-3 sr-checklist">
  <div><span class="sr-checklist-title"> &nbsp;</span></div>
    {{#each seoB as |rule|}}
      <p>
        <i class="icon-check"></i>
        <span class="sr-rule-title">{{rule.title}}</span>
        <span class="sr-rule-score">{{rule.score}}</span>
      </p>
    {{/each}}
  </div>
  <div class="col-3 sr-checklist score">
    <div class="sr-final-score">74</div>
    <div class="sr-final-score__label">SEO Score</div>
  </div>
</div>
{{/liquid-if}}
